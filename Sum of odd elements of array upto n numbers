#include<stdio.h>
#include<conio.h>
int main(){
int a[20], n,i,odd;
clrscr();
printf("Enter How Many NO.(Max upto 20):");
scanf("%d",&n);
for(i=0;i<n;i++){
printf("Enter %d th Element of An Array :",i+1);
scanf("%d",&a[i]);
}
odd=0;
for(i=0;i<n;i++){
if(a[i]%2==1){
odd=odd+a[i];
}
}
printf("The Sum Of All Odd element of an array = %d",odd);
getch();
return 0;
}

